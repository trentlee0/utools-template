{
  "name": "utools-utils",
  "description": "The common utils for uTools",
  "version": "1.0.7",
  "author": "Trent0",
  "scripts": {
    "build": "del-cli dist && tsc",
    "release": "npm run build && npm publish"
  },
  "main": "dist/index.js",
  "type": "commonjs",
  "exports": {
    ".": "./dist/index.js",
    "./common": "./dist/common.js",
    "./command": "./dist/command.js",
    "./storage": "./dist/storage.js",
    "./template": "./dist/template.js",
    "./type": "./dist/type/index.js"
  },
  "types": "./dist",
  "typesVersions": {
    "*": {
      "common": [
        "dist/common.d.ts"
      ],
      "command": [
        "dist/command.d.ts"
      ],
      "storage": [
        "dist/storage.d.ts"
      ],
      "template": [
        "dist/template.d.ts"
      ],
      "type": [
        "dist/type/index.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trentlee0/utools-utils.git"
  },
  "keywords": [
    "utools",
    "utils",
    "typescript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trentlee0/utools-utils/issues"
  },
  "homepage": "https://github.com/trentlee0/utools-utils#readme",
  "devDependencies": {
    "@types/node": "^16.11.7",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "utools-api-types": "^3.0.0"
  }
}
