{
  "name": "utools-utils",
  "description": "The common utils for uTools",
  "version": "1.1.7",
  "author": "Trent Lee",
  "scripts": {
    "build": "tsup src/index.ts src/preload.ts --dts --format cjs,esm --clean",
    "release": "npm run build && npm publish"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./preload": {
      "types": "./dist/preload.d.ts",
      "require": "./dist/preload.js",
      "import": "./dist/preload.mjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "preload": [
        "dist/preload.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trentlee0/utools-utils.git"
  },
  "keywords": [
    "utools",
    "utils",
    "typescript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trentlee0/utools-utils/issues"
  },
  "homepage": "https://github.com/trentlee0/utools-utils#readme",
  "devDependencies": {
    "@types/node": "^16.11.7",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "utools-api-types": "^3.0.0"
  }
}
